  /**
   * @brief It performs equality check between a string strings of characters.
   * @param[in] other The string to compare the character with.
   * @return The result of the comparison.
   *
   * @note It needs to be a c-string compatible type in order to make sure
   *       we find a null terminator.
   * @note If any of the strings is null, it returns false.
   *    If both are null, it returns true.
   */
  operator [[internal_linkage, inline]] ==(other: *const char) bool
  {
    let this = self as *const char;
    if !self && !other 
      { return true; }
    if !self || !other 
      { return false; } 
    let mut i = 0;
    while this[i] != 0 && other[i] != 0 
    {
      if this[i] != other[i] 
      {
        return false;
      }
      i = i + 1;
    }
  }
}

class [[__internal__]] *const <T> {
  pub:
    unsafe operator [[inline, llvm_function]] +(self: *const T, other: i64) *const T
    { 
      %0 = getelementptr {=T}, {=*const T} %self, i64 %other
      ret {=*const T} %0
    } 

    unsafe fn [[inline, llvm_function]] as_ref(self: *const T) &T 
    {
      ret {=*const T} %self
    }

    operator [[__internal__]] =(self: *const T, other: *const T) *const T {}

    fn [[inline, llvm_function]] is_null(self: *const T) bool
    {
      %0 = icmp eq {=*const T} %self, null
      ret i1 %0
    }

    fn [[inline]] unchecked_get(self: *const T, index: i32) &T
    {unsafe{
      return (self + index).as_ref();
    }}

    operator [[inline, llvm_function(sanitise_void_return)]] [](self: *const T, index: i64) T
    { 
      %0 = getelementptr inbounds {=T}, {=*const T} %self, i64 0
      %1 = load {=T}, {=*const T} %0, align 1
      ret {=T} %1
    }
    operator !(self: *const T) bool { return self.is_null(); }
}

