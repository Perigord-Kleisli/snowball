use [[use_macro]] Core::Assert;

namespace tests {

fn [[test]] empty() i32 {
    let v = new Vector<i32>();
    return v.size() == 0;
}

fn [[test(expect = 5)]] push() i32 {
    let mut v = new Vector<i32>();
    let mut i = 0;
    while (i < 5) {
        v.push(i);
        i = i + 1;
    }
    return v.size();
}

fn [[test(expect = 20)]] insert() i32 {
    let mut v = new Vector<i32>();
    v.insert(1, 5);
    v.insert(1, 10);
    v.insert(1, 20);
    @assert(v.size() == 3);
    return v.at(1);
}

}
