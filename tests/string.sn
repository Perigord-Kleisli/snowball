use Core::System;

namespace tests {

fn [[test]] empty() i32 {
    let s = new String();
    return s.size() == 0;
}

fn [[test(expect = 5)]] size() i32 {
    let s = new String("hello");
    return s.size();
}

fn [[test]] equals() i32 {
    let s = new String("hello");
    return s == "hello".to_string();
}

fn [[test]] not_equals() i32 {
    let s = new String("hello");
    return s != "world".to_string();
}

fn [[test]] concat() i32 {
    let mut s = new String("hello");
    s = s + " world";
    return s == "hello world".to_string();
}

}
