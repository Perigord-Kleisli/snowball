
use Core::Char;
use Core::System;

macro assert(condition: expr) {
  if !@condition {
    System::println("BALLLLLLLLLLLLLLLLLLLS");
    //raise new AssertionError(@format("Assertion '{}' failed at {}", @FILE_LINE));
  }
}

namespace tests {

fn [[test]] isSpace() i32 {
    System::println(@assert(Char::isSpace('b')));
}

}

