use Core::System

class Hello {
pub:
    Hello(x: String) {}
// priv:
    virt fn hello() {
        System::println("hello")
    }

    fn hey() {
        System::println("hey")
    }
}

fn h(d: String) String {
    return d
}

pub fn main() i32 {
    let a = new Hello("");
    a.hello()
    a.hey()

}
