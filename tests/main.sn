use Core::CLib

class Parent {
    pub:
        let mut a: char;
        Parent(x: char) {
            self.a = x;
        }
}

class Child : Parent {
    pub:
        Child(balls: char = 'A') : super(balls) {
        }

        virt fn test() {
            CLib::putchar('L');
        }
}

pub fn main() i32 {
    let mut c = new Child('B');
    let mut x = new Child();

    CLib::putchar(c.a);
    CLib::putchar(x.a);
    c.test();
    c.test();
    CLib::putchar('S');
}