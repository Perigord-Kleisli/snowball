// Import the core library.
// This is required for the println function.
use Core::System;

class Balls {
  pub:
    Balls() {
      let mut i = 0;
      while i < 100 {
          i = i + 1;
          System::print("Hello, world!");
      }
    }
}

// The main function is the entry point of the program.
// It is called when the program is started.
pub fn main() i32 {
    new Balls();
}
