class X {
    pub:
        X() {}
        fn to_string() String {
            return "X".to_string();
        }
};

class Hello {
  pub:
    Hello() {}
    virt fn say() X {
        return new X{};
    }
}

pub fn main() i32 {
    let a = new Hello{};
    let mut i = 0;
    let mut x: X;
    while ( i < 10 ) {
        x = a.say();
        i = i + 1;
    }

    System::println(x)

//    let mut x = 0..10;
